{
    "cSpell.words": [
        "appsettings",
        "fontsource",
        "Fontsource",
        "nunito",
        "Nunito"
    ],
    "editor.codeActionsOnSave": [
        "source.addMissingImports",
        "source.fixAll",
        "source.organizeImports"
    ],
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "eslint.packageManager": "npm",
    "files.exclude": {
        "**/.husky": false,
        "**/.vscode": false
    },
    "editor.formatOnSave": true,
    "prettier.requireConfig": true
}
