{
    "cSpell.language": "en",
    "editor.codeActionsOnSave": [
        "source.addMissingImports",
        "source.fixAll",
        "source.organizeImports"
    ],
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "eslint.packageManager": "npm",
    "files.exclude": {
        ".nyc_output": true
    },
    "editor.formatOnSave": true,
    "prettier.requireConfig": true
}
