{
  "name": "blip-extension-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "dev": "npm run start",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "spell": "cspell lint .",
    "prepare": "husky install",
    "cypress": "cypress open --component",
    "test": "npm run test:cypress",
    "test:cypress": "cypress run --component",
    "test:coverage": "npm run test",
    "test:result": "nyc report --reporter=text && nyc report --reporter=text-summary",
    "check": "npm run lint:fix && npm run format && npm run spell && npm run test:coverage && npm run test:result && npm run build"
  },
  "dependencies": {
    "@fontsource/nunito-sans": "^5.0.8",
    "blip-ds": "^1.246.2",
    "blip-iframe": "^0.0.4",
    "i18next": "^23.4.4",
    "iframe-message-proxy": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@cspell/dict-es-es": "^2.2.4",
    "@cspell/dict-pt-br": "^2.1.4",
    "@cypress/code-coverage": "^3.10.4",
    "@cypress/instrument-cra": "^1.4.0",
    "@types/node": "^20.1.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.14",
    "cspell": "^7.0.0",
    "cypress": "^12.9.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.10.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "postcss": "^8.4.21",
    "prettier": "^3.0.2",
    "react-scripts": "^5.0.1",
    "tailwind-blip-ds": "^1.0.4",
    "tailwindcss": "^3.2.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "./src/**/*.*": "prettier --write",
    "./src/**/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "exclude": [
      "cypress/**/*.*",
      "**/*.d.ts",
      "**/*.cy.tsx",
      "**/*.cy.ts"
    ]
  },
  "cspell": {
    "import": [
      "@cspell/dict-pt-br/cspell-ext.json",
      "@cspell/dict-es-es/cspell-ext.json"
    ],
    "language": "en,es,pt,pt_BR",
    "ignorePaths": [
      "node_modules",
      "build",
      "dist",
      ".vscode",
      "coverage",
      "azure-pipelines.yml",
      "public",
      "charts",
      "cypress/fixtures"
    ],
    "words": [
      "nginx",
      "appsettings",
      "commitlint",
      "datetime",
      "degit",
      "firefox",
      "fontsource",
      "Fontsource",
      "Heloi",
      "heloineto",
      "iframe",
      "lcov",
      "Neto",
      "nunito",
      "Nunito",
      "prestart",
      "tailwindcss",
      "takenet"
    ]
  }
}
