{
  "name": "blip-extension-template",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "react-scripts build",
    "check": "npm run lint:fix && npm run format && npm run spell && npm run build && npm run test:coverage && npm run test:result",
    "cypress": "cypress open --component",
    "dev": "npm run start",
    "eject": "react-scripts eject",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "spell": "cspell lint .",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "test": "npm run test:cypress",
    "test:coverage": "npm run test",
    "test:cypress": "cypress run --component",
    "test:result": "nyc report --reporter=text && nyc report --reporter=text-summary"
  },
  "dependencies": {
    "@fontsource/nunito-sans": "^5.0.12",
    "blip-ds": "^1.277.1",
    "i18next": "^23.10.1",
    "iframe-message-proxy": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@cspell/dict-es-es": "^2.4.0",
    "@cspell/dict-pt-br": "^2.3.0",
    "@cypress/code-coverage": "^3.12.30",
    "@cypress/instrument-cra": "^1.4.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@vercel/style-guide": "^6.0.0",
    "autoprefixer": "^10.4.19",
    "cspell": "^8.6.0",
    "cypress": "^13.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "react-scripts": "^5.0.1",
    "tailwind-blip-ds": "^1.0.4",
    "tailwindcss": "^3.4.1"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "./src/**/*.*": "prettier --write",
    "./src/**/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "**/*.cy.ts",
      "**/*.cy.tsx",
      "**/*.d.ts",
      "cypress/**/*.*"
    ],
    "include": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "tailwindcss": {}
    }
  },
  "prettier": "@vercel/style-guide/prettier",
  "cspell": {
    "import": [
      "@cspell/dict-pt-br/cspell-ext.json",
      "@cspell/dict-es-es/cspell-ext.json"
    ],
    "language": "en,es,pt,pt_BR",
    "ignorePaths": [
      "node_modules",
      "build",
      "dist",
      ".vscode",
      "coverage",
      "azure-pipelines.yml",
      "public",
      "charts",
      "cypress/fixtures"
    ],
    "words": [
      "appsettings",
      "commitlint",
      "datetime",
      "degit",
      "firefox",
      "fontsource",
      "Fontsource",
      "Heloi",
      "heloineto",
      "iframe",
      "lcov",
      "Neto",
      "nginx",
      "noreferrer",
      "nunito",
      "Nunito",
      "prestart",
      "tailwindcss",
      "takenet"
    ]
  }
}
